        <Button\n                    form=\"addHolidayForm\"\n                    key=\"submit\"\n                    htmlType=\"submit\"\n                    type=\"primary\"\n                    loading={loading}\n                >\n                    Submit\n                </Button>,\n            ]}\n        >\n            <Form id=\"addHolidayForm\" form={form} onFinish={handleOk}>\n                <Form.Item\n                    name=\"date\"\n                    rules={[\n                        { required: true, message: \"Please enter valid date\" },\n                    ]}\n                >\n                    <DatePicker placeholder=\"Holiday Date\" />\n                </Form.Item>\n                <Form.Item\n                    name=\"reason\"\n                    style={{\n                        marginBottom: \"1rem\",\n                    }}\n                    rules={[\n                        {\n                            required: true,\n                            message: \"Please enter the reason!\",\n                        },\n                    ]}\n                >\n                    <Input placeholder=\"Reason\" />\n                </Form.Item>\n            </Form>\n        </Modal>\n    );\n};\n\nexport default AddHolidayModal;\n","/home/rohit/Projects/StackHack-2.0/client/src/layout/components/menuHR.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/layout/components/menuEmployee.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/pages/Auth/HR/Register/components/index.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/index.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/salary.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/password.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/dropdown.jsx",[],"/home/rohit/Projects/StackHack-2.0/clien