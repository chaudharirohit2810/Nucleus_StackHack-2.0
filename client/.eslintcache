        setModalVisible(false);\n        message.success({ content: \"New FAQ Added\" });\n    };\n\n    const handleError = () => {\n        setModalVisible(false);\n        message.error({ content: \"Holiday additon failed! Please try again\" });\n    };\n\n    return (\n        <div>\n            <AddHolidayModal\n                isModalVisible={isModalVisible}\n                handleSuccess={handleSuccess}\n                handleError={handleError}\n                handleCancel={() => setModalVisible(false)}\n            />\n            <Title level={2}>Holidays</Title>\n\n            <Row justify=\"end\">\n                {loading ? (\n                    <div style={{ marginBottom: \"1.5rem\" }}>\n                        <Skeleton.Button active />\n                    </div>\n                ) : holidays.length !== 0 ? (\n                    <Button\n                        type=\"primary\"\n                        style={{\n                            marginBottom: \"1.5rem\",\n                        }}\n                        onClick={() => setModalVisible(true)}\n                    >\n                        Add Holiday\n                    </Button>\n                ) : null}\n            </Row>\n\n            {loading &&\n                _.times(7, i => (\n                    <div key={i}>\n                        <Skeleton\n                            active\n                            loading={loading}\n                            title={{ width: \"80%\" }}\n                            paragraph={{ rows: 0 }}\n                        />\n                    </div>\n                ))}\n            {!loading &&\n                (holidays.length != 0 ? (\n                    <HolidayTimeline holidays={holidays} />\n                ) : (\n                    <Empty description=\"No Holidays Added\">\n                        <Button\n                            type=\"primary\"\n                            style={{\n                                marginBottom: \"1.5rem\",\n                            }}\n                            onClick={() => setModalVisible(true)}\n                        >\n                            Add Holiday\n                        </Button>\n                    </Empty>\n                ))}\n        </div>\n    );\n};\n\nexport default Holiday;\n","/home/rohit/Projects/StackHack-2.0/client/src/layout/components/footer.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/layout/components/header.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/layout/components/tabsHR.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/layout/components/drawer.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/layout/components/drawerMobile.jsx",[],"/home/rohit/Projects/StackHack-2.0/client/src/pages/HR/holiday/holidayTimeline.jsx",["207"],"import React from \"react\";\nimport { Typography, Divider, Timeline, Row, Tabs } from \"antd\";\nconst { Title } = Typography;\nconst { TabPane } = Tabs;\n\nconst HolidayTimeline = ({ holidays }) => {\n    let items = [];\n    let monthItems = [];\n\n    var groupBy = (list, keyGetter) => {\n        const map = new Map();\n        list.forEach(item => {\n            const key = keyGetter(item);\n            const collection = map.get(key);\n            if (!collection) {\n                map.set(key, [item]);\n            } else {\n                collection.push(item);\n            }\n        });\n        return map;\n    };\n\n    return (\n        <>\n            {groupBy(holidays, holiday => holiday.year).forEach(\n                (value, key) => {\n                    monthItems = [];\n                    items.push(\n                        <TabPane tab={key} key={key}>\n                            {groupBy(value, val => val.month).forEach(\n                                (value, key) => {\n                                    monthItems.push(\n                                        <div key={key}>\n                                            <Title\n                                                level={5}\n                                                style={{ marginTop: \"0\" }}\n                                            >\n                                                {key}\n                                            </Title>\n                                            <Divider\n                                                style={{\n                                                    marginTop: \"0\",\n                                                }}\n                                            />\n                                            <Timeline mode=\"left\">\n                                                {value.map((item, index) => (\n                                                    <Timeline.Item\n                                                        key={index}\n                                                        position=\"left\"\n                                                    >\n                                                        <strong\n                                                            style={{\n                                                                marginRight:\n                                                                    \"0.5rem\",\n                                                            }}\n                                                        >\n                                                            {`${item.days}, ${item.dateNumber} ${item.month}`}\n                                                            :\n                                                        </strong>\n                                                        {item.reason}\n                                                    </Timeline.Item>\n                                                ))}\n                                            </Timeline>\n                                        </div>\n                                    );\n                                }\n                            )}\n                            {monthItems}\n                        </TabPane>\n                    );\n                }\n            )}\n            <Tabs>{items}</Tabs>\n        </>\n    );\n};\n\nexport default HolidayTimeline;\n","/home/rohit/Projects/StackHack-2.0/client/src/pages/Employee/calendarHeader.jsx",["208"],"import React from \"react\";\nimport { Row, Col, Typography, Select, Radio, Button, Space } from \"antd\";\n\nconst CalendarHeader = ({\n    value,\n    type,\n    onChange,\n    onTypeChange,\n    markAttendance,\n    buttonDisabled,\n}) => {\n    const start = 0;\n    const end = 12;\n    const monthOptions = [];\n\n    const current = value.clone();\n    const localeData = value.localeData();\n    const months = [];\n    for (let i = 0; i < 12; i++) {\n        current.month(i);\n        months.push(localeData.monthsShort(current));\n    }\n\n    for (let index = start; index < end; index++) {\n        monthOptions.push(\n            <Select.Option className=\"month-item\" key={`${index}`}>\n                {months[index]}\n            </Select.Option>\n        );\n    }\n    const month = value.month();\n\n    const year = value.year();\n    const options = [];\n    for (let i = year - 10; i < year + 10; i += 1) {\n        options.push(\n            <Select.Option key={i} value={i} className=\"year-item\">\n                {i}\n            </Select.Option>\n        );\n    }\n    return (\n        <div style={{ padding: 8 }}>\n            <Typography.Title level={4}>\n                Attendance: {months[month]} {String(year)}\n            </Typography.Title>\n            <Col>\n                <Row gutter={16}>\n                    <Col>\n                        <Radio.Group\n                            size=\"small\"\n                            onChange={e => onTypeChange(e.target.value)}\n                            value={type}\n                        >\n                            <Radio.Button value=\"month\">Month</Radio.Button>\n                            <Radio.Button value=\"year\">Year</Radio.Button>\n                        </Radio.Group>\n                    </Col>\n                    <Col>\n                        <Select\n                            size=\"small\"\n                            dropdownMatchSelectWidth={false}\n                            className=\"my-year-select\"\n                            onChange={newYear => {\n                                const now = value.clone().year(newYear);\n                                onChange(now);\n                            }}\n                            value={String(year)}\n                        >\n                            {options}\n                        </Select>\n                    </Col>\n                    <Col>\n                        <Select\n                            size=\"small\"\n                            dropdownMatchSelectWidth={false}\n                            value={String(month)}\n                            onChange={selectedMonth => {\n                                const newValue = value.clone();\n                                newValue.month(parseInt(selectedMonth, 10));\n                                onChange(newValue);\n                            }}\n                        >\n                            {monthOptions}\n                        </Select>\n                    </Col>\n                </Row>\n            </Col>\n            <Col style={{ marginTop: \"0.75rem\" }}>\n                <Button\n                    type=\"primary\"\n                    onClick={markAttendance}\n                    disabled={buttonDisabled}\n                >\n                    Mark Today's Attendance\n                </Button>\n            </Col>\n        </div>\n    );\n};\n\nexport default CalendarHeader;\n","/home/rohit/Projects/StackHack-2.0/client/src/pages/HR/holiday/addHolidayModal.jsx",["209"],"import React, { useState }