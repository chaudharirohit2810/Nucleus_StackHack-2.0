ancel = () => {\n        form.resetFields();\n        handleCancel();\n    };\n\n    return (\n        <Modal\n            title=\"Add New FAQ\"\n            visible={isModalVisible}\n            onCancel={onCancel}\n            footer={[\n                <Button key=\"cancel\" onClick={onCancel}>\n                    Cancel\n                </Button>,\n                <Button\n                    form=\"addHolidayForm\"\n                    key=\"submit\"\n                    htmlType=\"submit\"\n                    type=\"primary\"\n                    loading={loading}\n                >\n                    Submit\n                </Button>,\n            ]}\n        >\n            <Form id=\"addHolidayForm\" form={form} onFinish={handleOk}>\n                <Form.Item\n                    name=\"date\"\n                    rules={[\n                        { required: true, message: \"Please enter valid date\" },\n                    ]}\n                >\n                    <DatePicker placeholder=\"Holiday Date\" />\n                </Form.Item>\n                <Form.Item\n                    name=\"reason\"\n                    style={{\n                        marginBottom: \"1rem\",\n                    }}\n                    rules={[\n                        {\n                            required: true,\n                            message: \"Please enter the reason!\",\n                        },\n                    ]}\n                >\n                    <Input placeholder=\"Reason\" />\n                </Form.Item>\n            </Form>\n        </Modal>\n    );\n};\n\nexport default AddHolidayModal;\n","/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/faq/addFAQModal.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/HR/Register/components/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/employee/