function monthCellRender(value) {\n        const num = getMonthData(value);\n        return null;\n    }\n\n    const markAttendance = () => {\n        message.loading({\n            content: \"Marking Your Attendance....\",\n            key: \"attendance\",\n        });\n\n        setTimeout(() => {\n            message.success({\n                content: \"Attendance marked\",\n                key: \"attendance\",\n            });\n        }, 1000);\n    };\n\n    return (\n        <div style={{ maxWidth: \"80vw\", margin: \"0px auto\" }}>\n            <Calendar\n                dateFullCellRender={dateFullCellRender}\n                monthCellRender={monthCellRender}\n                fullscreen={false}\n                headerRender={props => (\n                    <CalendarHeader\n                        {...props}\n                        markAttendance={markAttendance}\n                    />\n                )}\n            />\n        </div>\n    );\n};\n\nexport default Attendance;\n","/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/faq.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/home.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/holiday.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/policy.jsx",["176"],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/tabsHR.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/drawerMobile.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/faq/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/drawer.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/footer.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/leaveTable.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/header.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/calendarHeader.jsx",["177"],"import React from \"react\";\nimport { Row, Col, Typography, Select, Radio, Button, Space } from \"antd\";\n\nconst CalendarHeader = ({\n    value,\n    type,\n    onChange,\n    onTypeChange,\n    markAttendance,\n}) => {\n    const start = 0;\n    const end = 12;\n    const monthOptions = [];\n\n    const current = value.clone();\n    const localeData = value.localeData();\n    const months = [];\n    for (let i = 0; i < 12; i++) {\n        current.month(i);\n        months.push(localeData.monthsShort(current));\n    }\n\n    for (let index = start; index < end; index++) {\n        monthOptions.push(\n            <Select.Option className=\"month-item\" key={`${index}`}>\n                {months[index]}\n            </Select.Option>\n        );\n    }\n    const month = value.month();\n\n    const year = value.year();\n    const options = [];\n    for (let i = year - 10; i < year + 10; i += 1) {\n        options.push(\n            <Select.Option key={i} value={i} className=\"year-item\">\n                {i}\n            </Select.Option>\n        );\n    }\n    return (\n        <div style={{ padding: 8 }}>\n            <Typography.Title level={4}>\n                Attendance: {months[month]} {String(year)}\n            </Typography.Title>\n            <Col>\n                <Row gutter={16}>\n                    <Col>\n                        <Radio.Group\n                            size=\"small\"\n                            onChange={e => onTypeChange(e.target.value)}\n                            value={type}\n                        >\n                            <Radio.Button value=\"month\">Month</Radio.Button>\n                            <Radio.Button value=\"year\">Year</Radio.Button>\n                        </Radio.Group>\n                    </Col>\n                    <Col>\n                        <Select\n                            size=\"small\"\n                            dropdownMatchSelectWidth={false}\n                            className=\"my-year-select\"\n                            onChange={newYear => {\n                                const now = value.clone().year(newYear);\n                                onChange(now);\n                            }}\n                            value={String(year)}\n                        >\n                            {options}\n                        </Select>\n                    </Col>\n                    <Col>\n                        <Select\n                            size=\"small\"\n                            dropdownMatchSelectWidth={false}\n                            value={String(month)}\n                            onChange={selectedMonth => {\n                                const newValue = value.clone();\n                                newValue.month(parseInt(selectedMonth, 10));\n                                onChange(newValue);\n                            }}\n                        >\n                            {monthOptions}\n                        </Select>\n                    </Col>\n                </Row>\n            </Col>\n            <Col style={{ marginTop: \"0.75rem\" }}>\n                <Button type=\"primary\" onClick={markAttendance}>\n                    Mark Today's Attendance\n                </Button>\n            </Col>\n        </div>\n    );\n};\n\nexport default CalendarHeader;\n","/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/HR/Register/components/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/menuHR.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/menuEmployee.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/faq/addFAQModal.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Employee/submitLeave.jsx",["178","179","180"],"import React from \"react\";\nimport { Form, Input, Button, DatePicker, Layout } from \"ant