=\"month-item\" key={`${index}`}>\n                {months[index]}\n            </Select.Option>\n        );\n    }\n    const month = value.month();\n\n    const year = value.year();\n    const options = [];\n    for (let i = year - 10; i < year + 10; i += 1) {\n        options.push(\n            <Select.Option key={i} value={i} className=\"year-item\">\n                {i}\n            </Select.Option>\n        );\n    }\n    return (\n        <div style={{ padding: 8 }}>\n            <Typography.Title level={4}>\n                Attendance: {months[month]} {String(year)}\n            </Typography.Title>\n            <Col>\n                <Row gutter={16}>\n                    <Col>\n                        <Radio.Group\n                            size=\"small\"\n                            onChange={e => onTypeChange(e.target.value)}\n                            value={type}\n                        >\n                            <Radio.Button value=\"month\">Month</Radio.Button>\n                            <Radio.Button value=\"year\">Year</Radio.Button>\n                        </Radio.Group>\n                    </Col>\n                    <Col>\n                        <Select\n                            size=\"small\"\n                            dropdownMatchSelectWidth={false}\n                            className=\"my-year-select\"\n                            onChange={newYear => {\n                                const now = value.clone().year(newYear);\n                                onChange(now);\n                            }}\n                            value={String(year)}\n                        >\n                            {options}\n                        </Select>\n                    </Col>\n                    <Col>\n                        <Select\n                            size=\"small\"\n                            dropdownMatchSelectWidth={false}\n                            value={String(month)}\n                            onChange={selectedMonth => {\n                                const newValue = value.clone();\n                                newValue.month(parseInt(selectedMonth, 10));\n                                onChange(newValue);\n                            }}\n                        >\n                            {monthOptions}\n                        </Select>\n                    </Col>\n                </Row>\n            </Col>\n            <Col style={{ marginTop: \"0.75rem\" }}>\n                <Button\n                    type=\"primary\"\n                    onClick={markAttendance}\n                    disabled={buttonDisabled}\n                >\n                    Mark Today's Attendance\n                </Button>\n            </Col>\n        </div>\n    );\n};\n\nexport default CalendarHeader;\n","/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/holiday/addHolidayModal.jsx",["218"],"import React, { useState } from \"react\";\nimport { Form, Input, Button, Modal, DatePicker } from \"antd\";\nimport axios from \"axios\";\nimport { backendURL } from \"../../../config\";\nconst { TextArea } = Input;\nconst AddHolidayModal = ({\n    isModalVisible,\n    handleSuccess,\n    handleCancel,\n    handleError,\n}) => {\n    var months = [\n        \"January\",\n        \"February\",\n        \"March\",\n        \"April\",\n        \"May\",\n        \"June\",\n        \"July\",\n        \"August\",\n        \"September\",\n        \"October\",\n        \"November\",\n        \"December\",\n    ];\n    var days = [\n        \"Sunday\",\n        \"Monday\",\n        \"Tuesday\",\n        \"Wednesday\",\n        \"Thursday\",\n        \"Friday\",\n        \"Saturday\",\n    ];\n    const [form] = Form.useForm();\n    const [loading, setLoading] = useState(false);\n\n    const handleOk = data => {\n        // setLoading(true);\n        data = {\n            ...data,\n            date: data[\"date\"]._d,\n            month: months[data[\"date\"].month()],\n            year: data[\"date\"].year(),\n            dateNumber: data[\"date\"].date(),\n            days: days[data[\"date\"].day()],\n        };\n\n        axios\n            .post(backendURL + \"/holidays/\", data)\n            .then(res => {\n                form.resetFields();\n                setLoading(false);\n                handleSuccess(data);\n            })\n            .catch(err => {\n                console.log(err);\n                form.resetFields();\n                setLoading(false);\n                handleError();\n            });\n    };\n\n    const onCancel = () => {\n        form.resetFields();\n        handleCancel();\n    };\n\n    return (\n        <Modal\n            title=\"Add New FAQ\"\n            visible={isModalVisible}\n            onCancel={onCancel}\n            footer={[\n                <Button key=\"cancel\" onClick={onCancel}>\n                    Cancel\n                </Button>,\n                <Button\n                    form=\"addHolidayForm\"\n                    key=\"submit\"\n                    htmlType=\"submit\"\n                    type=\"primary\"\n                    loading={loading}\n                >\n                    Submit\n                </Button>,\n            ]}\n        >\n            <Form id=\"addHolidayForm\" form={form} onFinish={handleOk}>\n                <Form.Item\n                    name=\"date\"\n                    rules={[\n                        { required: true, message: \"Please enter valid date\" },\n                    ]}\n                >\n                    <DatePicker placeholder=\"Holiday Date\" />\n                </Form.Item>\n                <Form.Item\n                    name=\"reason\"\n                    style={{\n                        marginBottom: \"1rem\",\n                    }}\n                    rules={[\n                        {\n                            required: true,\n                            message: \"Please enter the reason!\",\n                        },\n                    ]}\n                >\n                    <Input placeholder=\"Reason\" />\n                </Form.Item>\n            </Form>\n        </Modal>\n    );\n};\n\nexport default AddHolidayModal;\n","/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/faq/addFAQModal.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/HR/Register/components/index.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/employee/employeeContainer.jsx",["219"],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/menuHR.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/layout/components/menuEmployee.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/salary.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/HR/Register/components/password.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/password.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/Auth/Employee/Register/components/dropdown.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/employee/employeeCard.jsx",[],"/home/viraj007/Desktop/StackHack-2.0/client/src/pages/HR/employee/cardLoading.jsx",["220","221","222"],"import React from \"react\";\nimport { Card, Avatar, Row, Col, Skeleton, Space } from \"antd\";\nimport _ from \"lodash\";\nconst { Meta } = Card;\n\nconst CardLoading = ({ number }) => {\n    return (\n        <>\n            <Row>\n                {_.times(number, i => (\n                    <Card\n                        key={i}\n                        style={{\n                            maxWidth: 350,\n                            minWidth: 300,\n                            marginTop: 16,\n                            marginRight: 16,\n                        }}\n                        loading={tr